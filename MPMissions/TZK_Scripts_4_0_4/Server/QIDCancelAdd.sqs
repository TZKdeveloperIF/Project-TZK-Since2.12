; args: [qid, si]

_qid = _this select 0
_si = _this select 1


@!(mutexqidCancel)
mutexqidCancel = true
_inCheck = !( _qid in (qidCancel select _si) )
? _inCheck : (qidCancel select _si) set [count (qidCancel select _si), _qid]
mutexqidCancel = false


exit

