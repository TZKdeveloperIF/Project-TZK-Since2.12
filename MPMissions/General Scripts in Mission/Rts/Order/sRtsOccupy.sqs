; args: [clicked position, transport type info]
_top = TzkSelUnitsStack select TzkSelStackIdx
_c = count _top
? _c == 0: goto "Quit"

_pos = _top call preprocessFile "Player\Rts\CalcCentral.sqf"

_transpTypeInfo = _this select 1

_selectedUnits = +_top
[_selectedUnits, [_this select 0, _pos, _transpTypeInfo], "hOccupy.sqs", "Order"] exec "Rts\FrameWork\GenArrayCommand.sqs"

# Quit
false call preprocessFile "Player\Rts\Area.sqf"
false call preprocessFile "Player\Rts\Selection.sqf"