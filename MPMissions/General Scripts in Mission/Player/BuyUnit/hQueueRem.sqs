; args: [_qid, _si]

_qid = _this select 0
_si = _this select 1

? _si != siPlayer : exit

@!(mutexqPlayer)
mutexqPlayer = true
_found = false
_i = 0
_c = count qPlayer
while "_i<_c && !_found" do { if ( _qid == ((qPlayer select _i) select 0) ) then { qPlayer set [_i, -1]; _found = true }; _i=_i+1 }

qPlayer = qPlayer - [-1]

; player globalchat format["qPlayer = %1", qPlayer]
mutexqPlayer = false
