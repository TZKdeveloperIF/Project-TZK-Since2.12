; args: [type, si, giBuyer, giJoin, id]

_type = _this select 0
_si = _this select 1
_giBuyer = _this select 2
_giJoin = _this select 3
_id = _this select 4

_value = _type
_value = _value +  TotalUnitTypes *_id

_value2 = _giBuyer + GroupsNum*_giJoin + GroupsNum*GroupsNum*_si

[_value, _value2] exec "Player\BuyUnit\hQueueAdd.sqs"
publicExec format [{%1 exec "Player\BuyUnit\hQueueAdd.sqs"}, [_value, _value2]]