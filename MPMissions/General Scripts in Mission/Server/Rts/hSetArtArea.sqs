? not isServer: exit

; args: [x, y, w, h, dir, si, index]
_si = _this select 5
_idx = _this select 6
? _idx < 0 || _idx >= countTargetMarkers: [_si, "Invalid art marker index."] exec "Server\Msg\sSvrMsg2Cmd.sqs", exit

_markers = (call format ["TzkArtSvrArea%1", if (_si == si0) then {"W"} else {"E"}])
_info = _markers select _idx

{_info set [_x, _this select _x]} forEach [0,1,2,3,4]

_this exec "Player\Art\hArtAreaSet.sqs"
publicExec format [{%1 exec "Player\Art\hArtAreaSet.sqs"}, _this]