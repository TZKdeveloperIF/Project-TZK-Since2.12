? not isServer: exit

_netIds = _this select 0; _param = _this select 1
_c = count _netIds
? 0 == _c: exit

_this set [2, "Server\RtsOrder\ShootArea.sqs"]

@ not mutexTzkRtsW
	mutexTzkRtsW = true

	; 提供索引, 时间戳和阵营信息, 进行炮击区域的预分析
	; 之所以要求派发命令前执行而非设置区域后执行, 是为了避免提前划定炮击区域, 绕过建筑检查
	[_param select 2, _param select 4, _param select 5, true] call preprocessFile "Art\PreAnalysesArtArea.sqf"
	_this call preprocessFile "Server\Rts\DistributeOrder.sqf"

	mutexTzkRtsW = false