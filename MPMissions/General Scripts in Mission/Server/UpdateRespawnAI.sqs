; args: si

_si = _this

@ pvMatchRuleDefined

_mhq = mhq select _si

_objectRespawn = _mhq
[_objectRespawn, _si] exec "Net\sSetRespawnObj.sqs"

# Update
	~ 10
	_objectRespawn = sideRespawnObjects select _si
	? alive _objectRespawn: goto "Update"
	_objectRespawn = [_objectRespawn, _si] call funcGetNearestRespawnObject
	[_objectRespawn, _si] exec "Net\sSetRespawnObj.sqs"
	goto "Update"