; args: [vehicle, type]
? bool_TZK_Vanilla_Mode: exit

_vehicle = _this select 0; _scriptPath = "\TZK_Objects"; _scriptName = ""
_type = _this select 1

? "MHQW_xj400" countType [_vehicle] > 0: _scriptName = "M113Ambu", goto "END"
? "MHQE_xj400" countType [_vehicle] > 0: _scriptName = "BMPAmbu", _scriptPath = "\TZK_Config_4_0_6", goto "END"

# END
? _scriptName != "": [_vehicle, "G"] exec (_scriptPath + "\Scripts\texture\" + _scriptName + ".sqs")